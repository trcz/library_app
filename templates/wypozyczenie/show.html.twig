{% extends 'base.html.twig' %}

{% block title %}Wypożyczenie{% endblock %}

{% block body %}
    <h1>Wypożyczenie</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ wypozyczenie.id }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ wypozyczenie.status ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Data_wypozyczenia</th>
                <td>{{ wypozyczenie.dataWypozyczenia ? wypozyczenie.dataWypozyczenia|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('wypozyczenie_index') }}">Powrót do listy</a>

    <a href="{{ path('wypozyczenie_edit', {'id': wypozyczenie.id}) }}">Edytuj</a>

    {{ include('wypozyczenie/_delete_form.html.twig') }}
{% endblock %}
